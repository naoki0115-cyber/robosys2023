import sys
import math

def tonum(s):
    try:
        return int(s)
    except:
        return float(s)

numbers = []
for line in sys.stdin:
    num = tonum(line)
    numbers.append(num)

# 総和
sum_result = sum(numbers)
print(f"Sum: {sum_result if len(numbers) > 1 else numbers[0] if len(numbers) == 1 else 'No numbers provided.'}")

# 平均
if len(numbers) > 0:
    average = sum(numbers) / len(numbers)
    print(f"Average: {average}")
else:
    print("No numbers provided. Cannot calculate average.")

# 階乗
if len(numbers) > 0:
    factorial_result = math.prod(numbers)
    print(f"Factorial: {factorial_result}")
else:
    print("No numbers provided. Cannot calculate factorial.")

# 最大値
if len(numbers) > 0:
    max_value = max(numbers)
    print(f"Maximum: {max_value}")
else:
    print("No numbers provided. Cannot calculate maximum.")

# 最小値
if len(numbers) > 0:
    min_value = min(numbers)
    print(f"Minimum: {min_value}")
else:
    print("No numbers provided. Cannot calculate minimum.")
